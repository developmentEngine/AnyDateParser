{
  "name": "devengine/dateparser",
  "description": "PHP Parser of date strings in any format. Guesses the date format and creates a Carbon instance.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "devengine",
      "email": "i@devengine.ru"
    }
  ],
  "require": {
    "php": "^8.1",
    "nesbot/carbon": "^2.43",
    "ext-ctype": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Devengine\\AnyDateParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devengine\\Tests\\": "tests/"
    }
  }
}
